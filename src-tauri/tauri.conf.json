{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "productName": "ChatAny",
  "mainBinaryName": "ChatAny",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ChatAny",
        "width": 900,
        "height": 600,
        "minWidth": 900,
        "minHeight": 600,
        "hiddenTitle": true,
        "transparent": true,
        "titleBarStyle": "Overlay",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}